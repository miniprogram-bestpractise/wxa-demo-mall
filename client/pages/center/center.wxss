/** cartlist **/

.cartlist {
  width: 100%;
}

.cartbox {
  width: 100%;
  border-top: 1px solid #e5e5e5;
  background: #fff;
}

.supplier {
  padding: 0 13px;
  height: 41px;
  line-height: 41px;
  background: #fff;
  font-size: 14px;
  color: #000;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.i-default {
  flex: 1;
  height: 43px;
  color: #000;
  font-size: 14px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.status {
  padding: 5px;
  font-size: 14px;
}

.goodsbox {
  padding: 0 13px;
  background: #f8f8f8;
}

.goods {
  width: 100%;
  height: 109px;
  display: flex;
  flex-direction: row;
}

.djimgbox {
  margin: 11px 9px 0 0;
  width: 85px;
  height: 85px;
  border: 1px solid #e5e5e5;
  overflow: hidden;
}

.good-link {
  width: 85px;
  height: 85px;
}

.djimg {
  width: 85px;
  height: 85px;
}

.goodinfo {
  flex: 1;
  margin-top: 11px;
  width: 170px;
  overflow: hidden;
}

.infobox {
  width: 100%;
  font-size: 13px;
  color: #000;
  line-height: 16px;
  height: 32px;
  overflow: hidden;
  display: flex;
  flex-direction: row;
}

.good-name {
  flex: 1;
  font-size: 13px;
  color: #000;
  line-height: 16px;
  height: 32px;
  overflow: hidden;
}

.good-price {
  width: 100px;
  text-align: right;
  font-size: 12px;
}

.price-red {
  font-size: 16px;
}

.sizebox {
  margin-top: 1px;
  margin-bottom: 1px;
  font-size: 12px;
  color: #7c7c7c;
  line-height: 23px;
  position: relative;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: flex;
  flex-direction: row;
}

.sizeinfo {
  flex: 1;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  line-height: 14px;
}

.good-num {
  font-size: 14px;
}

.itembox {
  width: 100%;
  height: 32px;
  line-height: 32px;
  background: #fff;
}

.bg-gray {
  background: #f8f8f8;
  height: 41px;
  line-height: 41px;
}

.itembtr {
  margin: 0 13px;
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #7c7c7c;
}

.itemtxt {
  width: 30%;
  text-align: left;
  font-size: 14px;
  position: relative;
  font-weight: normal;
}

.itemnum {
  flex: 1;
  text-align: right;
  font-size: 14px;
}

.moneybox {
  width: 100%;
  background: #fff;
  height: 43px;
  line-height: 43px;
  border-bottom: 1px solid #e5e5e5;
  font-size: 12px;
  color: #383838;
}

.money-txt {
  text-align: right;
  padding-right: 13px;
}

.pl6 {
  padding-left: 6px;
}

.orderbtn {
  width: 100%;
  background: #fff;
  height: 43px;
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.itemblank {
  flex: 1;
}

.btn-gray {
  margin-right: 9px;
  width: 82px;
  height: 28px;
  line-height: 28px;
  text-align: center;
  font-size: 13px;
  color: #383838;
  border: 1px solid #aaa;
  border-radius: 4px;
}

.btn-client {
  display: inline-block;
  width: 60px;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
  margin-top: 0;
  margin-left: 0;
  border: 1px solid #fff;
}

.btn-red {
  margin-right: 9px;
  width: 74px;
  height: 26px;
  line-height: 26px;
  text-align: center;
  font-size: 13px;
  color: #fff;
  border: 1px solid #f7545f;
  border-radius: 4px;
}

/** page2 **/

.collectlist {
  width: 100%;
  background: #fff;
}

.collectbox {
  padding: 0 13px;
  height: 109px;
  display: flex;
  flex-direction: row;
  background: #fff;
}

.boximg {
  margin: 11px 9px 0 0;
  width: 90px;
  height: 90px;
  border: 1px solid #e5e5e5;
  overflow: hidden;
}

.collectinfo {
  flex: 1;
  margin-top: 30px;
  border-bottom: 1px solid #e5e5e5;
  line-height: 26px;
}

.collectinfo:last-child {
  border-bottom: none;
}

.infoname {
  flex: 1;
  font-size: 13px;
  color: #000;
  line-height: 16px;
  height: 32px;
}

.goodpri {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.colprice {
  flex: 1;
  margin-top: 10px;
  color: #000;
  width: 60px;
}

/**page3**/

.yhqlist {
  padding: 0 10px;
}

.yhqbox {
  margin: 10px auto;
  width: 100%;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  position: relative;
}

.yhqbg {
  width: 100%;
}

.yqhimg {
  width: 100%;
}

.yhqcont {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 64px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.yhqinfo {
  flex: 1;
  padding: 0 0 0 20%;
}

.yhqtit {
  font-size: 15px;
  color: #000;
  height: 20px;
  margin-bottom: 2px;
}

.yhqtime {
  color: #7c7c7c;
}

.yhqmoney {
  padding: 4px 10px 0 0;
  width: 67px;
  height: auto;
  text-align: right;
  color: #fff;
}

.ft24 {
  font-size: 24px;
}

.money {
  margin-bottom: 2px;
  font-size: 15px;
  height: 25px;
  line-height: 25px;
}

.condition {
  font-size: 11px;
  line-height: 12px;
}

