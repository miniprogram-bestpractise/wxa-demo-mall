/**order.wxss**/

.container {
  padding-bottom: 60px;
}

.userbox {
  margin-bottom: 10px;
  width: 100%;
  height: 97px;
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  flex-direction: row;
}

.userimg {
  margin: 14px 0 14px 11px;
  width: 66px;
  height: 66px;
  position: relative;
}

.imgbox {
  width: 64px;
  height: 64px;
  border: 2px solid #ddd;
  border-radius: 50%;
  overflow: hidden;
}

.headimg {
  width: 64px;
  height: 64px;
}

.head-ico {
  width: 13px;
  height: 13px;
  position: absolute;
  right: 6px;
  bottom: 0;
  z-index: 1;
}

.ico-qq {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.ico-weixin {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.userinfo {
  flex: 1;
  margin: 14px 0 0 9px;
}

.nick {
  margin: 10px 0 8px;
  font-size: 16px;
  color: #383838;
  /* height: 18px; */
}

.authorize {
  width: 60px;
  height: 30px;
  font-size: 12px;
  margin-left: 0;
  margin-top: 0;
  margin-bottom: 5px;
  background-color: #f7545f;
  color: #fff;
}

.login {
  width: 60px;
  height: 30px;
  font-size: 12px;
  margin-left: 0;
  margin-top: 0;
  margin-bottom: 5px;
  background-color: #f7545f;
  color: #fff;
}

.logout {
  display: inline-block;
  width: 60px;
  height: 30px;
  font-size: 12px;
  margin-left: 0;
  margin-top: 0;
  margin-right: 5px;
  background-color: #7c7c7c;
  color: #fff;
}

.myjudou {
  font-size: 12px;
  color: #7c7c7c;
}

.helpbox {
  margin: 20px 16px;
  width: 60px;
}

.faq-link {
  margin: 0 0 14px;
  width: 53px;
  height: 20px;
  color: #7c7c7c;
  line-height: 20px;
  position: relative;
}

.faq-link::after {
  content: "";
  height: 11px;
  width: 11px;
  background: url(data:image/png;base64,) no-repeat;
  position: absolute;
  top: 4px;
  right: -14px;
  z-index: 1;
  background-size: 100%;
}

.address-link {
  width: 53px;
  height: 20px;
  color: #7c7c7c;
  line-height: 20px;
  position: relative;
}

.address-link::after {
  content: "";
  height: 14px;
  width: 8px;
  background: url(data:image/png;base64,) no-repeat;
  position: absolute;
  bottom: 4px;
  right: -14px;
  z-index: 1;
  background-size: 100%;
}

