/**order.wxss**/
.container{
    padding-bottom: 60px;
}
.addressbox {
    width: 100%;
    height: 90px;
    margin-bottom: 9px;
    font-size: 15px;
    color: #000;
    background: url(data:image/jpeg;base64,) no-repeat;
    background-size: 100%;
}
.pagelink{
    width: 100%;
    height: 100%;
    color: #000;
}
.namebox {
    padding: 14px 0 8px;
    color: #000;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.person{
    flex: 1;
    padding-left: 33px;
    color: #000;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 15px;
    font-weight: normal;
}
.tel{
    padding-right: 35px;
}
.address {
    margin-bottom: 13px;
    padding: 0 25px 0 35px;
    font-size: 14px;
    color: #7c7c7c;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
}

/** cartlist **/
.cartlist{
    width: 100%;
}
.cartbox {
    width: 100%;
    border-top: 1px solid #e5e5e5;
    background: #fff;
}
.supplier{
    padding: 0 13px;
    height: 41px;
    line-height: 41px;
    background: #f8f8f8;
    font-size: 14px;
    color: #000;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.i-default{
    flex: 1;
    height: 43px;
    color: #000;
    font-size: 14px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.goodsbox {
    padding: 0 13px;
}
.goods{
    width: 100%;
    height: 109px;
    display: flex;
    flex-direction: row;
}
.imgbox {
    margin: 11px 9px 0 0;
    width: 85px;
    height: 85px;
    border: 1px solid #e5e5e5;
    overflow: hidden;
}
.good-link{
    width: 85px;
    height: 85px;
}
.djimg{
    width: 85px;
    height: 85px;
}
.goodinfo {
    flex: 1;
    margin-top: 11px;
    width: 170px;
    overflow: hidden;
}
.goodname {
    min-width: 170px;
    width: auto;
    max-width: 264px;
    font-size: 13px;
    color: #000;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
}
.sizebox{
    margin-top: 1px;
    margin-bottom: 1px;
    font-size: 12px;
    color: #7c7c7c;
    height: 23px;
    line-height: 23px;
    position: relative;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.box-color{
    padding-right: 6px;
}
.box-size{
    padding-right: 6px;
}
.sizebox-edit {
    background: #f1f1f1;
    min-width: 170px;
    width: auto;
    max-width: 264px;
}
.benefit{
    font-size: 12px;
    color: #7c7c7c;
    height: 10px;
    line-height: 10px;
    position: relative;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.goodpri {
    margin-top: 3px;
    position: relative;
    height: 15px;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.good-price{
    font-size: 12px;
    color: #f7545f;
    flex: 1;
}
.price-red{
    font-size: 16px;
}
.good-num{
    font-size: 14px;
    color: #383838;
}

.itembox{
    width: 100%;
    line-height: 47px;
    background: #fff;
}
.btr{
    border-bottom: 1px solid #e5e5e5;
}
.itembtr{
    margin: 0 13px;
    height: 47px;
    border-top: 1px solid #e5e5e5;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.nbtr{
    border-top: none;
}
.itemtxt{
    flex: 1;
    text-align: left;
    font-size: 14px;
    color: #000;
    width: 50%;
    position: relative;
    font-weight: normal;
}
.remarkbox{
    margin: 10px auto 0;
    width: 100%;
    height: 44px;
    border-top: 1px solid #dcdcdc;
    border-bottom: 1px solid #dcdcdc;
    background: #fff;
    line-height: 44px;
    font-size: 14px;
    color: #1b1b1b;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.remark-lab{
    padding-left: 10px;
    width: 56px;
}
.section{
    flex: 1;
    padding:0 10px 0 16px;
    height: 30px;
    overflow: hidden;
    line-height: 30px;
}
.remark-txt{
    height: 100%;
    width: 100%;
}
.btbr {
    margin-top:10px;
    width: 100%;
    border-bottom: 1px solid #dcdcdc;
    border-top: 1px solid #dcdcdc;
}
.yhq {
    position: relative;
    color: #383838;
}
.i-radio{
    margin-right: 5px;
    width: 15px;
    height: 15px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.i-checked{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.weixin{
    text-indent: 40px;
}
.weixin::after {
    content: "";
    position: absolute;
    left: 0;
    top: 13px;
    width: 33.5px;
    height: 20px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.cft{
    text-indent: 40px;
}
.cft::after {
    content: "";
    position: absolute;
    left: 0;
    top: 13px;
    width: 33.5px;
    height: 20px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.union{
    text-indent: 40px;
}
.union::after {
    content: "";
    position: absolute;
    left: 0;
    top: 13px;
    width: 33.5px;
    height: 20px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
/*btnbox*/
.cart-btnbox {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 49px;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
    box-shadow: 0 -2px 10px #e5e5e5;
    z-index: 3;
}
.balance{
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.totalpri{
    flex: 1;
    padding: 0 9px 0 13px;
    text-align: right;
}
.prinum{
    height: 14px;
    line-height: 14px;
    font-size: 15px;
    color: #383838;
}
.pritip{
    margin-top: 5px;
    font-size: 12px;
    color: #7c7c7c;
    text-align: right;
}
.cart-btn{
    width: 120px;
    height: 49px;
    line-height: 49px;
    text-align: center;
    background: #f7545f;
    font-size: 16px;
    color: #fff;
}
