/**order.wxss**/
.container{
    padding-bottom: 60px;
}
.orderzt{
    width: 100%;
    border-bottom: 1px solid #e5e5e5;
    margin-bottom: 10px;
    height: 59px;
    line-height: 59px;
    background: #fff;
    font-size: 15px;
    color: #000;
}
.zt-label{
    padding-left: 13px;
}

/** cartlist **/
.cartlist{
    width: 100%;
}
.cartbox {
    width: 100%;
    border-top: 1px solid #e5e5e5;
    background: #fff;
}
.supplier{
    padding: 0 13px;
    height: 41px;
    line-height: 41px;
    background: #f8f8f8;
    font-size: 14px;
    color: #000;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.i-default{
    flex: 1;
    height: 43px;
    color: #000;
    font-size: 14px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.goodsbox {
    padding: 0 13px;
}
.goods{
    width: 100%;
    height: 109px;
    display: flex;
    flex-direction: row;
}
.imgbox {
    margin: 11px 9px 0 0;
    width: 85px;
    height: 85px;
    border: 1px solid #e5e5e5;
    overflow: hidden;
}
.good-link{
    width: 85px;
    height: 85px;
}
.djimg{
    width: 85px;
    height: 85px;
}
.goodinfo {
    flex: 1;
    margin-top: 11px;
    width: 170px;
    overflow: hidden;
}
.infobox {
    width: 100%;
    font-size: 13px;
    color: #000;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
    display: flex;
    flex-direction: row;
}
.good-name{
    flex: 1;
    font-size: 13px;
    color: #000;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
}
.good-price{
    width: 100px;
    text-align: right;
    font-size: 12px;
}
.price-red{
    font-size: 16px;
}
.sizebox{
    margin-top: 1px;
    margin-bottom: 1px;
    font-size: 12px;
    color: #7c7c7c;
    line-height: 23px;
    position: relative;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
}
.sizeinfo{
    flex: 1;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    line-height: 14px;
}
.good-num{
    font-size: 14px;
}

.itembox{
    width: 100%;
    height: 32px;
    line-height: 32px;
    background: #fff;
}
.bg-gray{
    background: #f8f8f8;
    height: 41px;
    line-height: 41px;
}
.itembtr{
    margin: 0 13px;
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #7c7c7c;
}
.itemtxt{
    width: 30%;
    text-align: left;
    font-size: 14px;
    position: relative;
    font-weight: normal;
}
.itemnum{
    flex: 1;
    text-align: right;
    font-size: 12px;
}
.addressbox {
    width: 100%;
    height: 90px;
    font-size: 15px;
    color: #000;
    background: #fff;
    position: relative;
}
.addressbox::after{
    content: "";
    position: absolute;
    top: 32.5px;
    left: 10px;
    z-index: 1;
    width: 14px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.pagelink{
    width: 100%;
    height: 100%;
    color: #000;
}
.namebox {
    padding: 14px 0 8px;
    color: #000;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.person{
    flex: 1;
    padding-left: 33px;
    color: #000;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 15px;
    font-weight: normal;
}
.tel{
    padding-right: 35px;
}
.address {
    margin-bottom: 13px;
    padding: 0 25px 0 35px;
    font-size: 14px;
    color: #7c7c7c;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
}

.itemblank{
    flex: 1;
}
