/**address.wxss**/

.addrtip {
  width: 100%;
}

.nulladdbox {
  padding: 125px 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.i-location {
  width: 25px;
  height: 33px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.no-address {
  font-size: 16px;
  color: #a0a0a0;
  text-align: center;
  padding-top: 9px;
}

/*viewlist*/

.viewlist {
  width: 100%;
  padding-bottom: 60px;
}

.addressbox {
  margin-bottom: 9px;
  width: 100%;
  border-top: 1px solid #dcdcdc;
  border-bottom: 1px solid #dcdcdc;
  background: #fff;
  font-size: 15px;
  color: #1b1b1b;
}

.namebox {
  width: 100%;
  padding: 14px 0 8px;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 20px;
  line-height: 15px;
}

.person {
  flex: 1;
  padding-left: 10px;
}

.tel {
  padding-right: 10px;
}

.address {
  margin-bottom: 13px;
  padding: 0 10px;
  font-size: 14px;
  color: #444;
  line-height: 16px;
}

.actionbox {
  width: 100%;
  height: 42px;
  line-height: 42px;
  background: #fff;
  border-top: 1px solid #dcdcdc;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.i-default {
  padding-left: 10px;
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.i-radio {
  margin-right: 5px;
  width: 15px;
  height: 15px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.i-checked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.i-del {
  width: 50px;
  height: 43px;
  margin-right: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: end;
  font-size: 13px;
  color: #444;
}

.i-delico {
  margin-right: 5px;
  width: 18px;
  height: 18.5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.i-edit {
  margin-right: 14px;
  width: 50px;
  height: 43px;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 13px;
  color: #444;
}

.i-editico {
  margin-right: 5px;
  width: 17px;
  height: 18.5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

/*addr-btnbox*/

.addr-btnbox {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 49px;
  background: #f1f1f1;
}

.contpd {
  padding: 0 15px;
}

.btn-add {
  width: 100%;
  height: 43px;
  line-height: 43px;
  font-size: 16px;
}
