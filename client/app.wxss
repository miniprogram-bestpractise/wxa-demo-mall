.cartnum {
    position: absolute;
    right: -11px;
    top: -2px;
    width: 20px;
    height: 14px;
    line-height: 13px;
    text-align: center;
    font-size: 9px;
    color: #fff;
    font-style: normal;
    background: #ff3535;
    border-radius: 7px;
    border: 1px solid #fff;
    box-sizing: border-box;
}
.color-blue{
    color:blue;
}

.serpop {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 270px;
    height: 270px;
    margin-top: -135px;
    margin-left: -135px;
    z-index: 1000;
    background: #fff;
    border-radius: 6px;
}
.dialog-close {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 1001;
    width: 44px;
    height: 44px;
}
.btn-close {
    margin: 7px 0 0 15px;
    display: block;
    width: 21px;
    height: 21px;
    background: url(data:image/png;base64,) no-repeat;
    color: #3e3d3d;
    background-size: 100%;
    text-indent: -999em;
}
.sercont {
    width: 100%;
    height: 100%;
}
.title {
    padding: 0 15px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    font-size: 15px;
    color: #fff;
    background: #f7545f;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}
.seritem {
    padding: 0 15px;
    margin-top: 10px;
    height: auto;
    line-height: 20px;
    display: -webkit-box;
    display: box;
}
.sertips {
    padding: 15px 15px 0;
    font-size: 10px;
    color: #7c7c7c;
    line-height: 16px;
}
.pop-mask {
    display: -webkit-box;
    display: box;
    display: -ms-flexbox;
    -webkit-box-orient: horizontal;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    -ms-flex-pack: center;
    -ms-flex-align: center;
    background: rgba(0,0,0,.5);
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 1000;
}
