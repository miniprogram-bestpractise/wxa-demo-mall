/**app.wxss**/
/**reset**/
page{font:12px/1.5 arial,"\5FAE\8F6F\96C5\9ED1";background:#f1f1f1;color:#3e3d3d;}
.cf:after{content:" "; clear:both; height:0; visibility:hidden; display:block;}
.hide{display:none;}
.fl{float:left;}.fr{float:right;}
button{cursor:pointer;border:none;}
image{border:none;display:block;}
.fb{font-weight: bold;}
input, textarea, select {border: none;outline: none;font-family: tahoma,Microsoft yahei,"\5b8b\4f53",sans-serif;font-size: 14px;color:#7c7c7c;}

/*loading*/
.flower{font-size:11px;text-align:center;width:100%;padding-bottom:10px;padding-top:10px;line-height:20px;height:20px;color:#a0a0a0;}
.flower-icon{width:20px;height:20px;background:url(data:image/png;base64,) no-repeat;background-size:auto 20px;display:inline-block;vertical-align:middle;margin:-2px 5px 0 4px;-webkit-animation:flower-icon 1.2s linear infinite;-moz-animation:flower-icon 1.2s linear infinite;animation:flower-icon 1.2s linear infinite;}
@keyframes flower-icon{0%{-webkit-transform:rotate(0deg);}100%{-webkit-transform:rotate(360deg);}}
.bottom-tips{width:220px;margin:0 auto;padding-top:10px;padding-bottom:10px;text-align:center;font-size:11px;color:#a0a0a0;}

/*topnav*/
.topnav{position:fixed;bottom:60px;right:15px;z-index:100;width:42px;height:90px; transition: all .3s linear;display:none;}
.topnav-btn{margin-bottom:5px;display:block;width:42px;height:42px;background:url(data:image/png;base64,) no-repeat;background-size:100%;}
.btn-service{background:url(data:image/png;base64,) no-repeat;background-size:100%;}
.btn-return{background:url(data:image/png;base64,) no-repeat;background-size:100%;}

/*cont tips*/
.cont-tips{width:220px;margin:0 auto;padding-top:10px;padding-bottom:10px;text-align:center;font-size:11px;color:#a0a0a0;}
.list-more{width:100%;height:50px;background:#fff;border-bottom:1px solid #e5e5e5;position:relative;}
.list-more .expand{position:absolute;top:9px;left:50%;margin-left:-40px;width:80px;height:24px;text-align:left;line-height:24px;border:1px solid #afafaf;border-radius:25px;font-size:13px;color:#7c7c7c;text-indent:18px;}
.list-more .expand:after{content:"";height:6px;width:11px;background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAMBAMAAACU11D1AAAAFVBMVEUAAAC8vLy8vLy8vLy8vLy8vLy8vLzm4COtAAAABnRSTlMADMNvbo8KXP/9AAAAR0lEQVQI1yXJwQnAIABD0ZQuULpBLx3Bs+AMOoBo9h9ByM8pvK+xxMrWcM29vfV6wf50/a7h+SgBFgFOgAktTHCYABNgQg8fMSsQRm3agg0AAAAASUVORK5CYII=) no-repeat;position:absolute;bottom:9px;right:18px;z-index:1;background-size:100%;}
.close-up{width:100%;height:50px;background:#fff;border-bottom:1px solid #e5e5e5;position:relative;}
.close-up .expand{position:absolute;top:9px;left:50%;margin-left:-40px;width:80px;height:24px;text-align:left;line-height:24px;border:1px solid #afafaf;border-radius:25px;font-size:13px;color:#7c7c7c;text-indent:18px;}
.close-up .expand:after{content:"";height:6px;width:11px;background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAMBAMAAACU11D1AAAAFVBMVEUAAAC8vLy8vLy8vLy8vLy8vLy8vLzm4COtAAAABnRSTlMADMNvbo8KXP/9AAAAR0lEQVQI1yXJwQnAIABD0ZQuULpBLx3Bs+AMOoBo9h9ByM8pvK+xxMrWcM29vfV6wf50/a7h+SgBFgFOgAktTHCYABNgQg8fMSsQRm3agg0AAAAASUVORK5CYII=) no-repeat;transform:rotate(180deg);position:absolute;bottom:9px;right:18px;z-index:1;background-size:100%;}

/*order null*/
.querybox {
    margin: 0 auto;
    width: 220px;
    padding: 100px 0 50px;
    text-align: center;
    height: auto;
}
.querypic {
    margin: 0 auto;
    width: 87px;
    height: 62px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.querytips{
    margin: 18px auto 33px;
    width: 220px;
    min-height: 12px;
    height: auto;
    line-height: 16px;
    color: #7c7c7c;
    font-size: 12px;
}
.btn-query{
    margin: 0 auto;
    width: 135px;
    height: 37px;
    line-height: 37px;
    text-align: center;
    font-size: 15px;
    color: #f7545f;
    background: none;
    border: 1px solid #f7545f;
    border-radius: 5px;
}

/*comm*/
.comico{
    background:url(data:image/png;base64,) no-repeat;
    background-size:189px auto;
}
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0;
    box-sizing: border-box;
}
.red{
    color: #eb4c4c;
}
.ft12{
    font-size: 12px;
}
.ft14{
    font-size: 14px;
}
.ft15{
    font-size: 15px;
}
.ft16{
    font-size: 16px;
}
.ft18{
    font-size: 18px;
}
.btn-red{
    background: #f7545f;
    color: #fff;
    font-size: 16px;
    text-align: center;
}

/*menubar*/
.menubar{
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 100;
    width: 100%;
    height: 48px;
    line-height: 48px;
    display: flex;
    flex-direction: row;
    background: #f9f9f9;
    border-bottom: 1px solid #e5e5e5;
    border-top:1px solid #e5e5e5;
}
.menubox{
    flex: 1;
    align-items: center;
}
.menu-link{
    width: 100%;
    height: 48px;
}
.menuico{
